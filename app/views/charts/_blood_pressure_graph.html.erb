<div class="row">
  <div id="container" style="width:100%; height:400px;"></div>
  <script type="text/javascript">
    $(function () { 
        $('#container').highcharts({
            plotOptions: {
              series: {
                animation: false
              }
            },

            chart: {
                animation: false
            },
            title: {
                text: 'Blood Pressure'
            },
            xAxis: {
              categories: <%=raw @observation_set.map {|b| b.created_at.strftime("%H%M") } %>
            },
            series: [
              {
                showInLegend: false,               
                type: 'columnrange',
                data: <%=raw @observation_set.map {|b| [b.created_at.strftime("%H%M"), b.blood_pressure_max, b.blood_pressure_min] } %>
              }
            ]
        });
    });
  </script>
</div>
